program
    variable j : int ;
    variable n : int ;
    variable test : int ;


begin
    n <- 5;
    j <- 0;
    
    while j < 3 do
        begin
            j <- j+1 ;
        end 
    
    for j=0; j < n; j <- j+1 do
        begin	
            test <- j+n;
        end

    if n = 5 then
        begin 
            test <- n+j;
        end
    else 
        begin
            test <- 0;
        end

    print("n = %d\n", n);
    print("j = %d\n", j);
    print("test = %d\n", test);
end 