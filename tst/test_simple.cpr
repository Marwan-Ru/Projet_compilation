program
    variable i : float ;
    variable j : int ;


begin
    j <- 0;
    do 
        begin 
            j <- j + -1;  
        end 
    while j > -5

    i <- 1.0;
    if i > 0.0 then
    begin
        i <- i + -1.0;
        i <- i * 3.0;
    end
    
    print("j = %d\n", j);
    print("i = %f\n", i);
end 