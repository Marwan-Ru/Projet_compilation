program
    type date : struct
        jour : int ;
        mois : int ;
        annee : int ;
    endstruct ;

    type personne : struct
        numero : int ;
        age : int ;
        naissance : date ;
    endstruct ;

    type equipe : tab[1..3] of personne ;
    variable p : personne ;
    variable te : equipe ;
    variable b : bool ;
    variable i : float ;
    variable j : int ;


begin
    j <- 0;
    
    do 
        begin 
            j <- j + -2;  
        end 
    while j > -5
end 